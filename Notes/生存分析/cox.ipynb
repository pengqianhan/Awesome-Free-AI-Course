    "from lifelines import CoxPHFitter\n",
    "import pandas as pd\n",
    "data = pd.DataFrame({\n",
    "    'time': [3, 5, 4, 2],\n",
    "    'event': [1, 1, 0, 1],\n",
    "    'age': [30, 45, 25, 60],\n",
    "    'sex': [0, 1, 0, 1],\n",
    "    'height': [160, 175, 155, 180]\n",
    "})\n",
    "cph = CoxPHFitter()\n",
    "cph.fit(data, duration_col='time', event_col='event')\n",
    "cph.print_summary(style=\"ascii\")\n",
    "# cph.summary"
   ]
